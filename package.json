{
  "name": "littleslideshow",
  "version": "0.0.2",
  "description": "Little Slideshow is a simple slideshow developed in jQuery and works with Bootstrap.",
  "private": "true",
  "scripts": {
    "dev": "npm run development",
    "prod": "npm run production",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/little-apps/LittleSlideshow.git"
  },
  "author": "Little Apps",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/little-apps/LittleSlideshow/issues"
  },
  "homepage": "https://github.com/little-apps/LittleSlideshow#readme",
  "devDependencies": {
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "sass": "^1.49.7",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
